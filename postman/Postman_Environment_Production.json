{
  "id": "production-env",
  "name": "JEDx & Skills API - Production",
  "values": [
    {
      "key": "jedx_base_url",
      "value": "https://your-app-name.onrender.com/api/v1/jedx",
      "enabled": true
    },
    {
      "key": "skills_base_url",
      "value": "https://your-app-name.onrender.com/api/v1/skills",
      "enabled": true
    },
    {
      "key": "api_token",
      "value": "your-production-token",
      "enabled": true
    },
    {
      "key": "organization_uri",
      "value": "https://organization.example.com",
      "enabled": true
    }
  ],
  "_postman_variable_scope": "environment"
}
